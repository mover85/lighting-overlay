{
  "name": "lighting-overlay",
  "version": "2.0.0",
  "description": "",
  "homepage": "",
  "author": {
    "name": "Aaron Collins",
    "email": "mymover85@xtra.co.nz"
  },
  "scripts": {
    "start:dashboard": "npx parcel watch --target dashboard",
    "start:graphics": "npx parcel watch --target graphics",
    "start:extension": "npx parcel watch --target extension",
    "build:dashboard": "npx parcel build --target dashboard",
    "build:graphics": "npx parcel build --target graphics",
    "build:extension": "npx parcel build --target extension"
  },
  "browserslist": "last 2 chrome versions",
  "targets": {
    "dashboard": {
      "context": "browser",
      "source": "src/dashboard/live-controls.html",
      "distDir": "./dashboard/",
      "publicUrl": "./"
    },
    "graphics": {
      "context": "browser",
      "source": "src/graphics/index.html",
      "distDir": "./graphics/",
      "publicUrl": "./"
    },
    "extension": {
      "context": "node",
      "scopeHoist": false,
      "source": "src/extension/index.js",
      "distDir": "./extension/"
    }
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "live controls",
        "title": "Live Controls",
        "file": "live-controls.html",
        "headerColor": "#525F78",
        "fullbleed": true
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "assetCategories": [
      {
        "name": "albumart",
        "title": "Album art",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      }
    ]
  },
  "repository": "https://github.com/mover85/lighting-overlay.git",
  "license": "GPL-3.0",
  "dependencies": {
    "@material/mwc-button": "^0.27.0",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-image": "^3.0.2",
    "@wordpress/autop": "^3.18.0",
    "gsap": "^3.11.2",
    "haunted": "^5.0.0",
    "leo-profanity": "^1.6.1",
    "lit": "^2.3.1",
    "lodash.clone": "^4.5.0",
    "queue": "^6.0.2",
    "twemoji": "^14.0.2",
    "twitter-api-v2": "^1.12.7"
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.7.0"
  }
}
